// Function dispatch example (minimal runnable sample)

io :: import "std/io"

add :: fn(a: i32, b: i32) i32 {
    ret a + b
}

sub :: fn(a: i32, b: i32) i32 {
    ret a - b
}

main :: fn() {
    io.println("=== Function Dispatch ===")

    use_add := true
    result := if use_add { add(8, 3) } else { sub(8, 3) }
    io.println("result = {}", result)
}
