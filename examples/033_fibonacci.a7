// Fibonacci sequence

io :: import "std/io"

fib :: fn(n: i32) i32 {
    if n <= 1 {
        ret n
    }

    a := 0
    b := 1

    for i := 2; i <= n; i += 1 {
        next := a + b
        a = b
        b = next
    }

    ret b
}

main :: fn() {
    io.println("=== Fibonacci ===")

    for i := 0; i < 10; i += 1 {
        io.println("fib({}) = {}", i, fib(i))
    }
}
