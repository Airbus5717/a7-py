// String utility examples (deterministic)

io :: import "std/io"

main :: fn() {
    io.println("=== String Utils ===")

    text: string = "Hello"
    io.println("text = {}", text)
    io.println("first char = {}", 'H')

    greeting := "Hello, A7"
    io.println("greeting = {}", greeting)
    io.println("string utilities complete")
}
