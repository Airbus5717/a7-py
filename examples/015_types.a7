// Type examples in A7

io :: import "std/io"

main :: fn() {
    io.println("=== Types ===")

    i: i32 = 42
    f: f64 = 3.5
    b: bool = true
    c: char = 'Z'
    s: string = "typed"

    io.println("i = {}", i)
    io.println("f = {}", f)
    io.println("b = {}", b)
    io.println("c = {}", c)
    io.println("s = {}", s)
}
